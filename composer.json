{
    "name": "kkrot/kkrot",
    "description": "php frame",
    "type": "project",
    "keywords": [
        "php",
        "swoole",
        "framework"
    ],
    "php": ">=8.0",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "liutao",
            "email": "gg.cn.lt@gmail.com"
        }
    ],
    "require": {
        "ext-swoole": ">=4.5",
        "psr/container": "^1.0|^2.0",
        "psr/http-message": "~1.0",
        "laminas/laminas-mime": "^2.7",
        "nikic/fast-route": ">=1.3",
        "illuminate/collections": "9.x-dev",
        "psr/http-server-middleware": "^1.0",
        "ext-fileinfo": "*",
        "hyperf/di": "2.2.x-dev",
        "hyperf/contract": "2.2.x-dev",
        "hyperf/utils": "2.2.x-dev",
        "hyperf/config": "2.2.x-dev"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "swoole/ide-helper": "^4.5"
    },
    "autoload": {
        "psr-4": {
            "Kkrot\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "kkrot": {
            "config": "Kkrot\\ConfigProvider"
        }
    }
}
